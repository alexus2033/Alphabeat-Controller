<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset schemaVersion="1" mixxxVersion="">
    <info>
        <name>AlphaBeat-Mixxa</name>
        <author>Alexus2033</author>
        <description>Great mapping for AlphaBeat Board (based on Arduino Pro Micro)</description>
    </info>
    <controller id="">
        <scriptfiles>
            <file filename="AlphaBeat-scripts.js" functionprefix="ALPHABEAT"/>
        </scriptfiles>
        <controls>
            <control>
                <group>[Library]</group>
                <key>MoveVertical</key>
                <description>Rotary Encoder</description>
                <status>0xB0</status>
                <midino>0x2E</midino>
                <options>
                    <SelectKnob/>
                </options>
            </control>
            <control>
                <group>[Library]</group>
                <key>GoToItem</key>
                <description>Select Button</description>
                <status>0x90</status>
                <midino>0x37</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>play</key>
                <description>Start/Stop Deck 1</description>
                <status>0x90</status>
                <midino>0x30</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>play</key>
                <description>Start/Stop Deck 2</description>
                <status>0x90</status>
                <midino>0x31</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>cue_default</key>
                <status>0x90</status>
                <midino>0x38</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>cue_default</key>
                <status>0x90</status>
                <midino>0x39</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>volume</key>
                <description>Fader for Deck 1</description>
                <status>0xB0</status>
                <midino>0x37</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>volume</key>
                <description>Fader for Deck 2</description>
                <status>0xB0</status>
                <midino>0x36</midino>
                <options>
                    <normal/>
                </options>
            </control>
        </controls>
        <outputs><!-- OUTPUTS defined below............................... -->
          <output>
                <group>[Channel1]</group>
                <key>play_indicator</key>
                <!-- change blink behaviour: *Preferences > Interface > CUE MODE -->
                <description>Play LED</description>
                <status>0x90</status>
                <midino>0x01</midino>
                <minimum>0.5</minimum>
            </output>
        	<output>
                <group>[Channel2]</group>
                <key>play_indicator</key>
                <description>Play LED</description>
                <status>0x90</status>
                <midino>0x02</midino>
                <minimum>0.5</minimum>
            </output>
            <output>
                <group>[Channel1]</group>
                <key>end_of_track</key>
                <description>Pulsing Time Display 1</description>
                <status>0x90</status>
                <midino>0x3E</midino>
                <minimum>0.5</minimum>
            </output>
            <output>
                <group>[Channel2]</group>
                <key>end_of_track</key>
                <description>Pulsing Time Display 2</description>
                <status>0x90</status>
                <midino>0x3F</midino>
                <minimum>0.5</minimum>
            </output>
        </outputs>
    </controller>
</MixxxControllerPreset>
