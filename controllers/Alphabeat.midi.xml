<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset schemaVersion="1" mixxxVersion="">
    <info>
        <name>AlphaBeat-Mixxa</name>
        <author>Alexus2033</author>
        <description>Great mapping for AlphaBeat Board (based on Arduino Pro Micro)</description>
    </info>
    <controller id="">
        <scriptfiles>
            <file filename="AlphaBeat-scripts.js" functionprefix="ALPHABEAT"/>
        </scriptfiles>
        <controls>
            <control>
                <group>[Library]</group>
                <key>ALPHABEAT.selectItem</key>
                <description>Rotary Encoder</description>
                <status>0xB0</status>
                <midino>32</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Library]</group>
                <key>ALPHABEAT.selectItem</key>
                <description>Rotary Encoder</description>
                <status>0xB0</status>
                <midino>42</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>rate</key>
                <description>Set Speed</description>
                <status>0xB0</status>
                <midino>33</midino>
                <options>
                    <Rot64fast/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>rate</key>
                <description>Set Speed</description>
                <status>0xB0</status>
                <midino>43</midino>
                <options>
                    <Rot64fast/>
                </options>
            </control>
            <control>
                <group>[EqualizerRack1_[Channel1]_Effect1]</group>
                <key>parameter3</key>
                <description>Highpass-Filter Deck 1</description>
                <status>0xB0</status>
                <midino>34</midino>
                    <options>
                        <Rot64fast/>
                    </options>
            </control>
            <control>
                <group>[EqualizerRack1_[Channel2]_Effect1]</group>
                <key>parameter3</key>
                <description>Highpass-Filter Deck 2</description>
                <status>0xB0</status>
                <midino>44</midino>
                    <options>
                        <Rot64fast/>
                    </options>
            </control>
            <control>
                <group>[EqualizerRack1_[Channel1]_Effect1]</group>
                <key>parameter2</key>
                <description>Midpass-Filter Deck 1</description>
                <status>0xB0</status>
                <midino>35</midino>
                    <options>
                        <Rot64fast/>
                    </options>
            </control>
            <control>
                <group>[EqualizerRack1_[Channel2]_Effect1]</group>
                <key>parameter2</key>
                <description>Midpass-Filter Deck 2</description>
                <status>0xB0</status>
                <midino>45</midino>
                    <options>
                        <Rot64fast/>
                    </options>
            </control>
            <control>
                <group>[EqualizerRack1_[Channel1]_Effect1]</group>
                <key>parameter1</key>
                <description>Lowpass-Filter Deck 1</description>
                <status>0xB0</status>
                <midino>36</midino>
                    <options>
                        <Rot64fast/>
                    </options>
            </control>
            <control>
                <group>[EqualizerRack1_[Channel2]_Effect1]</group>
                <key>parameter1</key>
                <description>Lowpass-Filter Deck 2</description>
                <status>0xB0</status>
                <midino>46</midino>
                    <options>
                        <Rot64fast/>
                    </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>ALPHABEAT.changeVolume</key>
                <status>0xB0</status>
                <midino>37</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>ALPHABEAT.changeVolume</key>
                <status>0xB0</status>
                <midino>47</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
            <control>
                <group>[Library]</group>
                <key>GoToItem</key>
                <description>Select Button</description>
                <status>0x90</status>
                <midino>58</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>play</key>
                <description>Start/Stop Deck 1</description>
                <status>0x90</status>
                <midino>50</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>play</key>
                <description>Start/Stop Deck 2</description>
                <status>0x90</status>
                <midino>51</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>cue_default</key>
                <status>0x90</status>
                <midino>55</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>cue_default</key>
                <status>0x90</status>
                <midino>56</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>volume</key>
                <description>Fader for Deck 1</description>
                <status>0xB0</status>
                <midino>31</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel2]</group>
                <key>volume</key>
                <description>Fader for Deck 2</description>
                <status>0xB0</status>
                <midino>41</midino>
                <options>
                    <normal/>
                </options>
            </control>
        </controls>
        <outputs><!-- OUTPUTS defined below............................... -->
          <output>
                <group>[Channel1]</group>
                <key>play_indicator</key>
                <!-- change blink behaviour: *Preferences > Interface > CUE MODE -->
                <description>Play LED</description>
                <status>0x90</status>
                <midino>0x01</midino>
                <minimum>0.5</minimum>
            </output>
          <output>
                <group>[Channel2]</group>
                <key>play_indicator</key>
                <description>Play LED</description>
                <status>0x90</status>
                <midino>0x02</midino>
                <minimum>0.5</minimum>
          </output>
          <output>
                <group>[Channel1]</group>
                <key>end_of_track</key>
                <description>Pulsing Time Display 1</description>
                <status>0x90</status>
                <midino>0x3A</midino>
                <minimum>0.5</minimum>
            </output>
            <output>
                <group>[Channel2]</group>
                <key>end_of_track</key>
                <description>Pulsing Time Display 2</description>
                <status>0x90</status>
                <midino>0x3B</midino>
                <minimum>0.5</minimum>
            </output>
        </outputs>
    </controller>
</MixxxControllerPreset>
